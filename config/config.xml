<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!--
		If you're in a team environment and want to use your own config file,
		create one in this directory called "config-alt.xml" and override the values
		you need to. Take care not to add this file to your source-control system.
	-->

	<paths>
		<project></project>
		<aerial>src_php/lib/aerial</aerial>
		<libs>src_php/lib</libs>
		<amfphp>src_php/public_html/amfphp/core</amfphp>
		<doctrine>src_php/lib/doctrine</doctrine>
		<internal-services>src_php/lib/services</internal-services>
	</paths>
	<options>
		<debug-mode include-in-bootstrap="true">true</debug-mode>
		<server-url include-in-bootstrap="true"></server-url>
		<use-arraycollection>true</use-arraycollection>
		<use-authentication>false</use-authentication>
		<timezone>UTC</timezone>
	</options>
</config>
