<config>
  <!--
        If you're in a team environment and want to use your own config file,
        create one in this directory called "config-alt.xml" and override the values
        you need to. Take care not to add this file to your source-control system.
    -->
  <paths>
    <project></project>
    <aerial>src_php/lib/aerial</aerial>
    <libs>src_php/lib</libs>
    <amfphp>src_php/public_html/amfphp/core</amfphp>
    <doctrine>src_php/lib/doctrine</doctrine>
    <internal-services>src_php/lib/services</internal-services>
    <encryption>src_php/lib/aerial/encryption/</encryption>
  </paths>
  <code-generation>
    <php>src_php</php>
    <as3>src_flex</as3>
    <package>org.aerial</package>
    <bootstrap-package>org.aerial.bootstrap</bootstrap-package>
    <bootstrap-filename>Aerial</bootstrap-filename>
    <php-models-folder>vo</php-models-folder>
    <php-services-folder>services</php-services-folder>
    <as3-models-folder>vo</as3-models-folder>
    <as3-services-folder>services</as3-services-folder>
    <vo>VO</vo>
    <service>Service</service>
    <directory-mode>0664</directory-mode>
    <file-mode>0777</file-mode>
  </code-generation>
  <encryption>
    <use-encryption include-in-bootstrap="true">false</use-encryption>
  </encryption>
  <options>
    <debug-mode include-in-bootstrap="true">true</debug-mode>
    <server-url include-in-bootstrap="true"></server-url>
    <use-arraycollection>true</use-arraycollection>
    <collection-class>flex.messaging.io.ArrayCollection</collection-class>
    <use-authentication>false</use-authentication>
    <timezone>UTC</timezone>
  </options>
  <database use="development">
    <development>
      <engine>mysql</engine>
      <host>localhost</host>
      <database></database>
      <username></username>
      <password></password>
      <port>3306</port>
    </development>
  </database>
</config>